{
    "rewrites": [
        {
            "source": "/",
            "destination": "/app/index.html"
        },
        {
            "source": "/login",
            "destination": "/app/login.html"
        },
        {
            "source": "/signup",
            "destination": "/app/signup.html"
        },
        {
            "source": "/dashboard",
            "destination": "/app/dashboard.html"
        },
        {
            "source": "/(.*)",
            "destination": "/app/$1"
        }
    ]
}